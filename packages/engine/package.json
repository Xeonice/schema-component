{
  "name": "@schema-component/engine",
  "version": "0.1.0",
  "description": "Framework-agnostic data engine with DDD principles",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "engine",
    "core",
    "runtime",
    "ddd",
    "model",
    "repository",
    "mobx"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@schema-component/schema": "workspace:*",
    "eventemitter3": "^5.0.1",
    "inversify": "^6.0.3",
    "mobx": "^6.13.5",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "axios-mock-adapter": "^2.1.0",
    "typescript": "^5.6.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "axios": "^1.6.0"
  }
}
